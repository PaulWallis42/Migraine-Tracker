<div class='container'>
  <%= form_for @migraine do |f| %>
    <%= f.label :date %>
    <%= f.text_field :date, placeholder: "dd/mm/yyyy" %>
    <%= f.label :severity, "Severity: 1 (mild) to 10 (severe)" %>
    <%= f.select('severity', 1..10) %>
  </div>
  <div class='container'>
    <%= f.submit "Record Migraine "%>
  <% end %>
</div>
