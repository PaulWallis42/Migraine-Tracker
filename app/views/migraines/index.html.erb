<% if @migraines.any? %>
  <% @migraines.each do |migraine| %>
    <h3><%= "Migraine Date: #{migraine.date}" %></h2>
    <h3><%= "Migraine Severity #{migraine.severity}"%></h2>
    <h3><%= "Phenylalanine Level #{migraine.phen_level}"%></h2>
  <% end %>
<% else %>
  <h2>No Migraines Recorded</h2>
<% end %>

<%= link_to 'Record a Migraine', new_migraine_path %>
