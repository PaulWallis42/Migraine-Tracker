<% if @migraines.any? %>
  <% @migraines.each do |migraine| %>
    <h2><%= "Migraine Date: #{migraine.date}" %></h2>
    <h2><%= "Migraine Severity #{migraine.severity}"%></h2>
    <h2><%= "Phenylalanine Level #{migraine.phen_level}"%></h2>
  <% end %>
<% else %>
  <h2>No Migraines Recorded</h2>
<% end %>

<a href='#'>Record a Migraine</a>
