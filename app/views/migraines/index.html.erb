  <% if user_signed_in? %>
    <% if @migraines.any? %>
      <% @migraines.each do |migraine| %>
      <div class='container migraines'>
      <div class='row'>
              <%= "Migraine Date: #{migraine.date}" %>
              <%= "Migraine Severity: #{migraine.severity}"%>
              <%= "Phenylalanine Level: #{migraine.phen_level} grams"%>
            </div>
          </div>
        <% end %>
    <% else %>
        <div class='no-recorded'>
          <h2>No Migraines Recorded</h2>
        </div>
    <% end %>
  <% end %>
