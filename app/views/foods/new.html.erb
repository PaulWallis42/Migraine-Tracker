<%= form_for @food do |f| %>
  <%= f.label :name, "Food" %>
  <%= f.text_field :name, placeholder: "Cheese"%>
  <%= f.label :food_quant, "Quantity" %>
  <%= f.text_field :food_quant, placeholder: "grams"%>
  <%= f.label :phen_quant, "Phenylalanine Quantity" %>
  <%= f.text_field :phen_quant, placeholder: "grams" %>
  <%= f.submit "Submit Food" %>
<% end %>
